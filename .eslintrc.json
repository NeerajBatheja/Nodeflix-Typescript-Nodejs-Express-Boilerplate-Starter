{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "overrides": [
      {
        "files": ["**/*.ts"],
        "rules": {
          "no-unused-vars": 2,
          "no-console": 2,
          "@typescript-eslint/no-floating-promises": 2
        }
      }
    ]
  }
  